<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>History - Data per Minute</title>
</head>
<body>
  <h1>Data per Minute</h1>
  <table border="1">
    <thead>
      <tr>
        <th>Location</th>
        <th>Temperature (Â°C)</th>
        <th>Humidity (%)</th>
        <th>Rain</th>
        <th>Rain Output</th>
        <th>Distance (cm)</th>
        <th>Status</th>
        <th>Timestamp</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach((entry) => { %>
        <tr>
          <td><%= entry.location %></td>
          <td><%= entry.tempC %></td>
          <td><%= entry.humidity %></td>
          <td><%= entry.rain %></td>
          <td><%= entry.rainOutput %></td>
          <td><%= entry.distance %></td>
          <td><%= entry.status %></td>
          <td><%= new Date(entry.time).toLocaleString() %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>